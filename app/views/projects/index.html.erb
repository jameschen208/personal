<h1 class="main_header">accomplishments</h1>
<h2 class="secondary_header">take a peek</h2>

<div id="posts_wrapper" class="skinny_wrapper wrapper_padding">
	<% @projects.each do |project| %>
		<div class="post">
			<p class="date"><%= project.created_at.strftime('%A, %B %d') %></p>
			<h2><a href="#"><%= link_to project.title, project %></a></h2>
			<hr>
		</div>
	<% end %>
</div>
